{
    "editor.codeLens": false,
    "editor.definitionLinkOpensInPeek": true,
    "editor.detectIndentation": true,
    "editor.fontFamily": "JetBrains Mono",
    "editor.fontLigatures": true,
    "editor.fontSize": 19,
    "editor.formatOnPaste": false,
    "editor.formatOnSave": false,
    "editor.inlayHints.enabled": "offUnlessPressed",
    "editor.guides.bracketPairs": true,
    "editor.multiCursorModifier": "ctrlCmd",
    "editor.rulers": [
        80,
    ],
    "editor.renderControlCharacters": false,
    "editor.renderWhitespace": "all",
    "editor.selectionClipboard": false,
    "editor.semanticHighlighting.enabled": true,
    "editor.semanticTokenColorCustomizations": {
        "rules": {
            "class": {
                "foreground": "#d533bb",
                "fontStyle": ""
            },
            "enum": {
                "foreground": "#e1afc3",
                "fontStyle": "underline"
            },
            "enumConstant": {
                "foreground": "#587d87",
                "fontStyle": "italic"
            },
            "function": {
                "foreground": "#e5b124",
                "fontStyle": ""
            },
            "macro": {
                "foreground": "#e79528",
                "fontStyle": ""
            },
            "method": {
                "foreground": "#57c2cc",
                "fontStyle": "italic"
            },
            "namespace": {
                "foreground": "#429921",
                "fontStyle": ""
            },
            "parameter": {
                "foreground": "#368896",
                "fontStyle": ""
            },
            "type":{
                "foreground": "#a04360",
                "fontStyle": ""
            },
            "variable": {
                "foreground": "#7ca6b7",
                "fontStyle": ""
            }
        }
    },
    "editor.snippetSuggestions": "top",
    "editor.tabSize": 2,
    "workbench.colorTheme": "Monokai",
    "workbench.iconTheme": "vscode-icons",
    "workbench.list.keyboardNavigation": "filter",
    "workbench.settings.editor": "json",
    "workbench.settings.useSplitJSON": true,
    "workbench.tree.indent": 16,
    "window.restoreWindows": "none",
    "window.titleBarStyle": "custom",
    "window.zoomLevel": 1,
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/.cache": true,
        "**/build": true,
        "**/build-*": true,
        "**/*.o": true,
        "**/*.d": true
    },
    "files.hotExit": "off",
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": false,
    "files.watcherExclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/.cache": true,
        "**/build": true,
        "**/build-*": true,
        "**/*.o": true,
        "**/*.d": true
    },
    "explorer.confirmDelete": false,
    "search.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/.cache": true,
        "**/build": true,
        "**/build-*": true,
        "**/*.o": true,
        "**/*.d": true
    },
    "search.smartCase": true,
    "search.useIgnoreFiles": false,
    "debug.inlineValues": "on",
    "extensions.ignoreRecommendations": true,
    "problems.decorations.enabled": false,
    "telemetry.telemetryLevel": "off",
    "gitlens.codeLens.authors.enabled": false,
    "gitlens.codeLens.recentChange.enabled": false,
    "gitlens.currentLine.enabled": false,
    "gitlens.keymap": "alternate",
    "python.formatting.provider": "yapf",
    "python.formatting.yapfPath": "/usr/bin/yapf2",
    "python.jediEnabled": true,
    "python.jediPath": "/usr/lib/python2.7/site-packages",
    "python.linting.enabled": true,
    "python.linting.lintOnSave": true,
    "python.linting.maxNumberOfProblems": 100,
    "python.linting.pep8Args": [],
    "python.linting.pep8Enabled": true,
    "python.linting.pep8Path": "pycodestyle",
    "python.linting.pylintEnabled": false,
    "python.linting.pep8CategorySeverity.E": "Error",
    "python.linting.pep8CategorySeverity.W": "Warning",
    "python.pythonPath": "/usr/bin/python2",
    "git.confirmSync": false,
    "cmake.configureOnOpen": false,
    "mesonbuild.buildFolder": "build",
    "mesonbuild.configureOnOpen": false,
    "clangd.arguments": [
        "-log=info",
        "--completion-style=detailed",
        "--header-insertion=never"
    ],
    "rust-analyzer.checkOnSave.command": "clippy",
    "rust-analyzer.inlayHints.bindingModeHints.enable": true,
    "rust-analyzer.inlayHints.chainingHints.enable": true,
    "rust-analyzer.inlayHints.closingBraceHints.enable": true,
    "rust-analyzer.inlayHints.closingBraceHints.minLines": 25,
    "rust-analyzer.inlayHints.closureReturnTypeHints.enable": "always",
    "rust-analyzer.inlayHints.expressionAdjustmentHints.enable": "always",
    "rust-analyzer.inlayHints.lifetimeElisionHints.enable": "skip_trivial",
    "rust-analyzer.inlayHints.lifetimeElisionHints.useParameterNames": false,
    "rust-analyzer.inlayHints.maxLength": 25,
    "rust-analyzer.inlayHints.parameterHints.enable": true,
    "rust-analyzer.inlayHints.typeHints.enable": true,
    "rust-analyzer.inlayHints.typeHints.hideClosureInitialization": false,
    "rust-analyzer.inlayHints.typeHints.hideNamedConstructor": false,
    "rust-analyzer.procMacro.enable": true,
    "bashIde.path": "/usr/bin/bash-language-server",
    "shellcheck.customArgs": [
        "--shell=bash",
        "--external-sources"
    ],
    "shellcheck.exclude": [
        "SC2059"
    ],
    "shellcheck.run": "onSave",
    "shellformat.flag": "-i 2 -bn -sr -ci",
    "remote.SSH.showLoginTerminal": true,
    "remote.SSH.defaultExtensions": [
        "eamodio.gitlens",
        "ms-vscode.sublime-keybindings",
        "ryzngard.vscode-header-source",
        "shardulm94.trailing-spaces",
        "twxs.cmake",
        "vector-of-bool.cmake-tools",
        "webfreak.debug",
    ],
    "remote.containers.defaultExtensions": [
        "eamodio.gitlens",
        "ms-vscode.sublime-keybindings",
        "ryzngard.vscode-header-source",
        "shardulm94.trailing-spaces",
        "twxs.cmake",
        "vector-of-bool.cmake-tools",
        "webfreak.debug",
    ]
}
